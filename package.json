  "scripts": {
    "test": "mocha --opts ./mocha.opts",
    "build": "tsc",
    "test:watch": "mocha --opts ./mocha.opts --watch",
    "lint-src": "tslint ./src/**/*.ts",
    "start": "ts-node   ./src/index.ts",
    "node-start": "node ./dist/out-tsc/src/index.js",
    "start:serve": "npm run start && npm run serve-app",
    "ts-node": "ts-node",
    "lint-app": "tslint ./srcApp/**/*.ts",
    "test-app": "ng test",
    "build-app": "ng build && npm run test-app && npm run lint-app",
    "serve-app": "ng serve -port 4200 & powershell.exe -command ./StartApp.ps1",
    "build-all": "npm run build && npm run build-app",
    "lint": "npm run lint-src && npm run lint-app"
  },
